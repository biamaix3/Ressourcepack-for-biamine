{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:component",
    "component": "minecraft:custom_model_data",
    "cases": [
      {
        "when": { "floats": [11003] },
        "model": {
          "type": "minecraft:condition",
          "property": "minecraft:fishing_rod/cast",
          "on_true": {
            "type": "minecraft:model",
            "model": "fishing_rod_variants:item/bias_fishing_rod/bias_fishing_rod_cast"
          },
          "on_false": {
            "type": "minecraft:model",
            "model": "fishing_rod_variants:item/bias_fishing_rod/bias_fishing_rod"
          }
        }
      }
    ],
    "fallback": {
      "type": "minecraft:condition",
      "property": "minecraft:fishing_rod/cast",
      "on_true": {
        "type": "minecraft:model",
        "model": "minecraft:item/fishing_rod_cast"
      },
      "on_false": {
        "type": "minecraft:model",
        "model": "minecraft:item/fishing_rod"
      }
    }
  }
}
